	<queryString language="SQL">
		<![CDATA[SELECT r.ID_Rental, r.Tanggal_Rental, p.Nama AS Nama_Pelanggan, 
       k.Nama_konsol, dr.Durasi, r.Total_Harga, s.Tanggal_Selesai
FROM tbl_rental r
INNER JOIN tbl_pelanggan p ON r.ID_Pelanggan = p.ID_Pelanggan
INNER JOIN tbl_detail_rental dr ON r.ID_Rental = dr.ID_Rental
INNER JOIN tbl_konsol k ON dr.ID_Konsol = k.ID_Konsol
INNER JOIN tbl_selesai s ON dr.ID_Rental = s.ID_Rental
ORDER BY r.Tanggal_Rental DESC;]]>
	</queryString>
